<view class="mask" wx:if="{{mask}}">
    
</view>
<view class="main" wx:else>
    <view class="img" style="margin-bottom: 50rpx">
        <image src="{{imgsrc}}" style="height:200px"></image>
    </view>

    <van-divider  custom-style="margin:0px;" contentPosition="left">返回结果</van-divider>
    <view class="math_c">
        <towxml nodes="{{math}}"  />
    </view>

    <van-divider  custom-style="margin:0px;" contentPosition="left">算式答案</van-divider>
    <view class="math_c">
        <towxml nodes="{{result}}"  />
    </view>

    <view class="again">
      <view class="btn" bindtap="back">
          <image  src="/images/fanhui.png"></image>
          <text>重新识别</text>
      </view>
      <view class="edit" bindtap="edit">
          <image  src="/images/edit.png"></image>
          <text>编辑</text>
      </view>
    </view> 
</view>
