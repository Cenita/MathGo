page {
  width: 100%;
  height: 100%;
}
.mask {
  width: 100%;
  height: 100%;
  background:rgba(0,0,0,0.4);
}
.main {
  width: 100%;
}
.main .img {
  width: 100%;
  margin: 0 auto;
}
.img image {
  width: 100%;
  height: 100%;
}
.again {
  width: 100%;
  height: 120rpx;
  position: fixed;
  bottom: 0;
  left: 0;
  right: 0;
  display: flex;
  flex-direction: row;
  justify-content: space-around;
  background-color: #308bff;
  align-items: center;
}
.again .btn ,.again .edit {
  display: flex;
  flex-direction: column;
  width: 50%;
  height: 100rpx;
  justify-content: space-around;
  align-items: center;
}
.again image {
  width: 50rpx;
  height: 50rpx;
}
.again text {
  color: #fff;
  font-size: 28rpx;
}
.result {
  margin-top: 50rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
}
.result_icon{
  width: 50rpx;
  height: 50rpx;
  margin-left: 50rpx;
  margin-right: 20rpx;
}
.result text {
  font-size: 30rpx;
}
