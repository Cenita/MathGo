<view class="box">
    <image-cropper id="image-cropper"
  limit_move="true"
  width="200"
  height="200"
  imgSrc="{{src}}"
  max_width="{{imgWidth}}"
  max_height="{{imgHeight}}"
  img_width="{{imgWidth}}"
  img_height="{{imgHeight}}"
  bindload="cropperload"
  bindimageload="loadimage"
  bindtapcut="clickcut">
  </image-cropper>
</view>

<view class="type"> 
    <picker class="type_c" bindchange="bindPickerChange" value="{{index}}" range="{{array}}">
        <text>识别类型: {{array[index]}}</text>
        <image class="arr" src="/images/arr.png"></image>
    </picker>
</view>


