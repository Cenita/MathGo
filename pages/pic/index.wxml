<view class="box">
    <image-cropper id="image-cropper"
  limit_move="true"
  width="{{width}}"
  height="{{height}}"
  imgSrc="{{src}}"
  min_width="{{minWidth}}"
  min_height="{{minHeight}}"
  max_width="{{imgWidth}}"
  max_height="{{imgHeight}}"
  img_width="{{imgWidth}}"
  img_height="{{imgHeight}}"
  modeList="{{modeList}}"
  bindload="cropperload"
  bindimageload="loadimage"
  bindtapcut="clickcut"
  bindclose="returnToCamera"
  quality="0.8"
  cut_top="{{cut_top}}"
  cut_left="{{cut_left}}"
  bind:getIndex="getIndex"
  >
  </image-cropper>
</view>



